const groupedData = data.reduce((result, item) => {
  const date = item.date;
  if (!result[date]) {
    result[date] = {
      tickers: []
    };
  }
  result[date].tickers.push({ ticker: item.ticker });
  return result;
}, {});

// Mengonversi objek grup menjadi array
const transformed = Object.values(groupedData).map(item => ({
  date: item.date,
  tickers: item.tickers
}));
