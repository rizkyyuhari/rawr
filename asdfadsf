  <Table
    columns={columns}
    dataSource={data}
    pagination={{ showTotal }}
    footer={() => (
      <Space style={{ display: 'flex', justifyContent: 'space-between', alignItems: 'center' }}>
        <div>{showTotal(data.length)}</div>
        <div>
          <Table.Pagination />
        </div>
      </Space>
    )}
  />
